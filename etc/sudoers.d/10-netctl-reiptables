
# Members of @reconfigure_group@ group allowed to execute following commands:
%@reconfigure_group@	ALL = (root:root)        NOEXEC,NOPASSWD: /sbin/iptables-save
%@reconfigure_group@	ALL = (root:root)        NOEXEC,NOPASSWD: /sbin/iptables-restore
%@reconfigure_group@	ALL = (root:root)        NOEXEC,NOPASSWD: /sbin/ip6tables-save
%@reconfigure_group@	ALL = (root:root)        NOEXEC,NOPASSWD: /sbin/ip6tables-restore
